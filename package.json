{
  "name": "vscode-squirrel-re",
  "displayName": "squirrel_re",
  "description": "squirrel_re language support",
  "version": "1.0.0",
  "publisher": "r-ex",
  "icon": "acorn.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "squirrel",
    "nut"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "languages": [
        {
            "id": "squirrel",
            "aliases": [
                "Squirrel",
                "squirrel"
            ],
            "extensions": [
                ".nut",
                ".gnut"
            ],
            "configuration": "./language-configuration.json"
        }
    ],
    "grammars": [
        {
            "language": "squirrel",
            "scopeName": "rspn.squirrel",
            "path": "./syntaxes/squirrel.tmLanguage.json"
        }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r-ex/vscode-squirrel-re.git"
  }
}
